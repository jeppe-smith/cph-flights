  "devDependencies": {
    "concurrently": "^3.5.1",
    "vue-svg-loader": "^0.5.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cross-env": "^5.1.3",
    "fuse.js": "^3.2.0",
    "v-debounce": "^0.1.2"
  }
