{
  "name": "cph-flights",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "8.9.4"
  },
  "scripts": {
    "postinstall": "echo \"Installing dependencies. Exciting!\" && cd server && npm i --no-progress && cd ../client && npm i --no-progress",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "cross-env NODE_ENV=development concurrently -k -p \"[{name}]\" -n \"SERVER,CLIENT\" -c \"blue,green\" \"cd server && npm run dev\" \"cd client && npm run dev\"",
    "build": "cd client && npm run build",
    "start": "cross-env PORT=80 node server/index.js"
  },
  "repository": {
    "type": "git",
